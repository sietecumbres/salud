<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "Lista")}", tipo_insumos_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "Nuevo")}", new_tipo_insumo_path %></li>
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title">Tipos de Insumos</h2>
    <div class="inner">
      <table class="table">
        <tr>             
          <th class="first"></th>
          <th>Nombre</th>
          <th>Fabricante</th>
          <th>Presentaci√≥n</th>
          <th>Proveedor</th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @tipo_insumos.each do |tipo_insumo| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td></td>
          <td><%= tipo_insumo.nombre %></td>
          <td><%= tipo_insumo.fabricante_insumo.nombre %></td>
          <td><%= tipo_insumo.presentacion %></td>
          <td><%= tipo_insumo.proveedor_insumo.nombre %></td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Ver")}", tipo_insumo_path(tipo_insumo) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Editar")}", edit_tipo_insumo_path(tipo_insumo) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Eliminar")}", tipo_insumo_path(tipo_insumo), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Esta seguro?")}" %>
          </td>
        </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'layouts/sidebar') -%>
