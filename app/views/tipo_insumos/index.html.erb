<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", tipo_insumos_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_tipo_insumo_path %></li>
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> TipoInsumos</h2>
    <div class="inner">
      <table class="table">
        <tr>             
          <th class="first">ID</th>
                    <th>
            <%= t("activerecord.attributes.tipo_insumo.nombre", :default => t("activerecord.labels.nombre", :default => "Nombre")) %>
          </th>
                    <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @tipo_insumos.each do |tipo_insumo| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
            <%= tipo_insumo.id %>
          </td>
                    <td>
            <%= link_to tipo_insumo.nombre, tipo_insumo_path(tipo_insumo) %>
          </td>
                    <td>
            <%= tipo_insumo.created_at %>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", tipo_insumo_path(tipo_insumo) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_tipo_insumo_path(tipo_insumo) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", tipo_insumo_path(tipo_insumo), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
