
  <div class="group">
    <%= f.label :nombre, t("activerecord.attributes.tipo_insumo.nombre", :default => "Nombre"), :class => :label %>
    <%= f.text_field :nombre, :class => 'text_field' %>
  </div>

  <div class="group">
    <%= f.label :fabricante_insumo_id, t("activerecord.attributes.tipo_insumo.fabricante_insumo_id", :default => "Fabricante del insumo"), :class => :label %>
    <%= f.select :fabricante_insumo_id, options_for_select(FabricanteInsumo.all.collect{|fab| [fab.nombre, fab.id]}), {:include_blank => true} %>
  </div>

  <div class="group">
    <%= f.label :presentacion, t("activerecord.attributes.tipo_insumo.presentacion", :default => "Presentacion"), :class => :label %>
    <%= f.text_field :presentacion, :class => 'text_field' %>
  </div>

  <div class="group">
    <%= f.label :proveedor_id, t("activerecord.attributes.tipo_insumo.proveedor_id", :default => "Proveedor"), :class => :label %>
    <%= f.select :proveedor_insumo_id, options_for_select(ProveedorInsumo.all.collect{|p| [p.nombre, p.id]}), {:include_blank => true} %>
  </div>
        
<div class="group navform wat-cf">
  <button class="button" type="submit">
    <%= image_tag("web-app-theme/icons/tick.png", :alt => "#{t("web-app-theme.save", :default => "Guardar")}") %> <%= t("web-app-theme.save", :default => "Guardar") %>
  </button>  
  <%= link_to image_tag("web-app-theme/icons/cross.png", :alt => "#{t("web-app-theme.cancel", :default => "Cancelar")}") + " " + t("web-app-theme.cancel", :default => "Cancelar"), tipo_insumos_path, :class => "button" %>
</div>