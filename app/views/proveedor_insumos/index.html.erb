<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "Lista")}", proveedor_insumos_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "Nuevo")}", new_proveedor_insumo_path %></li>
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title">Proveedores de Insumos</h2>
    <div class="inner">
      <table class="table">
        <tr>             
          <th class="first"></th>
          <th>Nombre</th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @proveedor_insumos.each do |proveedor_insumo| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td></td>
          <td><%= proveedor_insumo.nombre %></td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.edit", :default => "Editar")}", edit_proveedor_insumo_path(proveedor_insumo), :class => "sexybutton sexysimple sexyblue sexysmall" %>
            <%= link_to "#{t("web-app-theme.delete", :default => "Eliminar")}", proveedor_insumo_path(proveedor_insumo), :class => "sexybutton sexysimple sexyblue sexysmall", :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Esta seguro?")}" %>
          </td>
        </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'layouts/sidebar') -%>
