<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "Lista")}", subtipo_equipos_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "Nuevo")}", new_subtipo_equipo_path %></li>
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title">Subtipos Equipos</h2>
    <div class="inner">
      <table class="table">
        <tr>             
          <th class="first"></th>
          <th><%= t("activerecord.attributes.subtipo_equipo.nombre", :default => t("activerecord.labels.nombre", :default => "Nombre")) %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @subtipo_equipos.each do |subtipo_equipo| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
          </td>
          <td>
            <%= subtipo_equipo.nombre %>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.edit", :default => "Editar")}", edit_subtipo_equipo_path(subtipo_equipo), :class => "sexybutton sexysimple sexyblue" %>
            <%= link_to "#{t("web-app-theme.delete", :default => "Eliminar")}", subtipo_equipo_path(subtipo_equipo), :class => "sexybutton sexysimple sexyblue", :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Esta seguro?")}" %>
          </td>
        </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'layouts/sidebar') -%>
