<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", tipos_insumo_pruebas_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_tipos_insumo_prueba_path %></li>
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> TiposInsumoPruebas</h2>
    <div class="inner">
      <table class="table">
        <tr>             
          <th class="first">ID</th>
                    <th>
            <%= t("activerecord.attributes.tipos_insumo_prueba.prueba_id", :default => t("activerecord.labels.prueba_id", :default => "Prueba_id")) %>
          </th>
                    <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @tipos_insumo_pruebas.each do |tipos_insumo_prueba| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
            <%= tipos_insumo_prueba.id %>
          </td>
                    <td>
            <%= link_to tipos_insumo_prueba.prueba_id, tipos_insumo_prueba_path(tipos_insumo_prueba) %>
          </td>
                    <td>
            <%= tipos_insumo_prueba.created_at %>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", tipos_insumo_prueba_path(tipos_insumo_prueba) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_tipos_insumo_prueba_path(tipos_insumo_prueba) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", tipos_insumo_prueba_path(tipos_insumo_prueba), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
