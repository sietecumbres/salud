<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "Lista")}", almacenamientos_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "Nuevo")}", new_almacenamiento_path %></li>
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title">Almacenamientos</h2>
    <div class="inner">
      <table class="table">
        <tr>
          <th class="first"></th>
          <th>Insumo</th>
          <th>Zona</th>
          <th>Vitrina</th>
          <th>Posici√≥n</th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @almacenamientos.each do |almacenamiento| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td></td>
          <td><%= almacenamiento.insumo_id %></td>
          <td><%= almacenamiento.zona %></td>
          <td><%= almacenamiento.vitrina %></td>
          <td><%= almacenamiento.posicion %></td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Ver")}", almacenamiento_path(almacenamiento) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Editar")}", edit_almacenamiento_path(almacenamiento) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Eliminar")}", almacenamiento_path(almacenamiento), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Esta seguro?")}" %>
          </td>
        </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'layouts/sidebar') -%>
