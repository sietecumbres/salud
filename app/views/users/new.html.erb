<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "Listado de Usuarios", users_path  %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title">Nuevo Usuario</h2>
    <div class="inner">
      <%= form_for :user, :url => users_path, :html => { :class => 'form to-validate', :multipart => true } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'layouts/sidebar') -%>