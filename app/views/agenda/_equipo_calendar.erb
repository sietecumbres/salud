<br/>
<span>Seleccione el a&ntilde;o:</span>
<%= select_tag :year_for_agenda, options_for_select((Date.today.year..(Date.today.year + 10)).to_a), {:include_blank => 'seleccionar'} %>
<br/>
<span>Agenda para el a&ntilde;o: <%= year %></span>
<div class="block">
  <div class="secondary-navigation">
  </div>
  <div class="content">
    <h2 class="title">Calendario</h2>
    <div class="inner">
      <div class="inventario">
        <table class="table">
          <!-- This is the months header -->
          <thead>
            <tr>
              <th class="first"></th>
              <th>Enero</th>
              <th>Febrero</th>
              <th>Marzo</th>
              <th>Abril</th>
              <th>Mayo</th>
              <th>Junio</th>
              <th>Julio</th>
              <th>Agosto</th>
              <th>Septiembre</th>
              <th>Octubre</th>
              <th>Noviembre</th>
              <th class="last">Diciembre</th>
            </tr>
          </thead>
          <tbody>
            <%= build_equipo_calendar_row(equipo, year)  %>
          </tbody>
        </table>
        </div>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
      </div>
    </div>
  </div>
</div>